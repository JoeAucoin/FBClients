	@ClientID INT,
	

	
	@CreatedByUserID INT
	 
AS 
BEGIN 
	SET NOCOUNT ON;


INSERT INTO [dbo].[GIBS_FBClientsIncomeAndExpense]
           ([IeType]
           ,[IeDescription]
           ,[IeAmount]
           ,[ClientID]
           ,[CreatedOnDate]
           ,[CreatedByUserID]
           ,[LastModifiedByUserID]
           ,[LastModifiedOnDate])
     VALUES
     (
          @IeType,
          @IeDescription,
          @IeAmount,
          @ClientID,
          GetDate(),
          @CreatedByUserID,
          @CreatedByUserID,
          GetDate()
          
          )	



select SCOPE_IDENTITY()
	
END



