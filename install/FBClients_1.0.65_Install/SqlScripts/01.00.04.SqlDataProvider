 ALTER procedure [dbo].[GIBS_FBClients_TrueFalseQuestions_List]

	@ClientID int

as
BEGIN 

	     
	SELECT TfID
		  ,TfQuestion
		  ,TfAnswer
		  ,ClientID
		  ,CreatedOnDate
		  ,CreatedByUserID
		  ,LastModifiedByUserID
		  ,LastModifiedOnDate
	  FROM dbo.GIBS_FBClientsTrueFalseQuestions
		
		where  GIBS_FBClientsTrueFalseQuestions.ClientID = @ClientID
		

END




